<Controls:MetroWindow  xmlns:Controls="http://metro.mahapps.com/winfx/xaml/controls" x:Class="AngelTest.Configurator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                       mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button
             x:Name="UsersButton" Click="UsersButton_Click" Width="100" Height="100" Style="{StaticResource MahApps.Styles.Button.MetroSquare.Accent}">
            <Controls:FontIcon FontSize="55" FontFamily ="Segoe MDL2 Assets" Glyph="&#xE77B;"/>
        </Button>
        <Button
             x:Name="SettingsButton" Click="SettingsButton_Click" Width="100" Height="100" Style="{StaticResource MahApps.Styles.Button.MetroSquare.Accent}" Margin="100,160,0,159">
            <Controls:FontIcon FontSize="55" FontFamily ="Segoe MDL2 Assets" Glyph="&#xE713;"/>
        </Button>
        <Frame x:Name="MainFr"/>
    </Grid>
</Controls:MetroWindow>
